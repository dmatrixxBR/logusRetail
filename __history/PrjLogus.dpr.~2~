program PrjLogus;

uses
  Vcl.Forms,
  untMain in 'untMain.pas' {frmMain},
  untData in 'untData.pas' {DM: TDataModule};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TfrmMain, frmMain);
  Application.CreateForm(TDM, DM);
  Application.Run;
end.
